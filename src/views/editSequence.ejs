<script>
	$(document).ready(function() {

	pageSetUp();
        var errorClass = 'invalid';
			var errorElement = 'em';
			
			var $addUserForm = $('#add-form').validate({
				errorClass	: errorClass,
				errorElement	: errorElement,
				highlight: function(element) {
			        $(element).parent().removeClass('state-success').addClass("state-error");
			        $(element).removeClass('valid');
			    },
			    unhighlight: function(element) {
			        $(element).parent().removeClass("state-error").addClass('state-success');
			        $(element).addClass('valid');
			    },

			// Rules for form validation
				rules : {
					estado: { required: true}
					,origenId: { digits: true, required: true}
					,mostraId: { digits: true, required: true}
					,hospedeiro: { required: true}
					,microorganismoIdSuspeito: { digits: true, required: true}
					,dataColecta: { required: true}
					,codigoPais: { required: true}
					,cidade: { required: true}
					,sequenciadorId: { digits: true, required: true}
					,servicoSaudeId: { digits: true, required: true}
				},
		
				messages : {
					estado: { required: "O estado e obligatorio"}
					,origenId: { digits: "O codigo do origen deve ser numerico", required: "O codigo do origen e obligatorio"}
					,mostraId: { digits: "O codigo da amostra deve ser numerico", required: "O codigo da amostra e obligatorio"}
					,hospedeiro: { required: "O codigo do origen e obligatorio"}
					,microorganismoIdSuspeito: { digits: "O codigo do MicroOrganismo deve ser numerico", required: "O codigo do MicroOrganismo e obligatorio"}
					,dataColecta: { required: "A data Colecta e obligatoria"}
					,codigoPais: { required: "O codigo do pais e obligatorio"}
					,cidade: { required: "A cidade e obligatorio"}
					,sequenciadorId: { digits: "O codigo do origen deve ser numerico", required: "O codigo do sequenciador e obligatorio"}
					,servicoSaudeId: { digits: "O codigo do origen deve ser numerico", required: "O codigo do servico de saude e obligatorio"}
				},
		
				// Do not change code below
				errorPlacement : function(error, element) {
					error.insertAfter(element.parent());
				}
			});

	
		$("[name='hospedeiro']").get(0).focus();
	
	});
	
</script>
